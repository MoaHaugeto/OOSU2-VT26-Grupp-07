<Window x:Class="OOSU2_VT26_Grupp_07.Presentationslager_WPF_.HanteraMedlemar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOSU2_VT26_Grupp_07.Presentationslager_WPF_"
        mc:Ignorable="d" FontSize="18" FontFamily="Lucida Bright"
        Title="HanteraMedlemmar" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="20"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="200"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Column="1" Grid.Row="1" Text="Hantera Medlemmar" FontSize="35" 
                   Grid.ColumnSpan="3" Margin="10"/>
        <Button x:Name="visaMedlemmarButton" Content="Visa Medlemmar" 
                Grid.Column="1" Grid.Row="2" Margin="5" Click="visaMedlemmarButton_Click"/>
        <Button x:Name="uppdateraMedlemmarButton" Content="Uppdatera Medlemmar" 
                Grid.Column="1" Grid.Row="3" Margin="5" Click="uppdateraMedlemmarButton_Click"/>
        <Button x:Name="registreraMedlemmarButton" Content="Registrera Medlemmar"
                Grid.Column="2" Grid.Row="2" Margin="5" Click="registreraMedlemmarButton_Click"/>
        <Button x:Name="återgåButton" Grid.Column="1" Grid.Row="5" Content="Tillbaka" Height="25" Click="återgåButton_Click"/>

    </Grid>
</Window>
